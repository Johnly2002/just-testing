<?xml version='1.0' encoding='UTF-8' standalone='no'?>
<!-- (C) Copyright 2020-2023 Hewlett Packard Enterprise Development LP -->
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">

    <groupId>com.hpe.ci.mgmt</groupId>
    <modelVersion>4.0.0</modelVersion>
    <properties>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <atlasDependencyVersion>8.70.9999999-SNAPSHOT</atlasDependencyVersion>
        <ras.version>8.70.0</ras.version>
    </properties>
    <version>${ras.version}-SNAPSHOT</version>
    <artifactId>redfish-server-adapter</artifactId>
    <description>PSRM Redfish Device Module Source</description>
    <packaging>pom</packaging>

    <organization>
        <name>Hewlett Packard Enterprise</name>
        <url>www.hpe.com</url>
    </organization>

    <parent>
        <groupId>org.springframework.boot</groupId>
        <artifactId>spring-boot-starter-parent</artifactId>
        <version>2.3.2.RELEASE</version>
        <relativePath/>
        <!-- lookup parent from repository -->
    </parent>


    <contributors>
        <contributor>
            <name>OV Components Team</name>
            <email>ov-components-se-in@hpe.com</email>
            <organization>HPE</organization>
        </contributor>
    </contributors>
    <build>
        <pluginManagement>
            <plugins>
                <plugin>
                    <artifactId>lifecycle-mapping</artifactId>
                    <groupId>org.eclipse.m2e</groupId>
                    <version>1.0.0</version>
                    <configuration>
                        <lifecycleMappingMetadata>
                            <pluginExecutions>
                                <pluginExecution>
                                    <pluginExecutionFilter>
                                        <artifactId>native2ascii-maven-plugin</artifactId>
                                        <groupId>org.codehaus.mojo</groupId>
                                        <versionRange>[1.0-beta-1,)</versionRange>
                                        <goals>
                                            <goal>native2ascii</goal>
                                        </goals>
                                    </pluginExecutionFilter>
                                    <action>
                                        <ignore/>
                                    </action>
                                </pluginExecution>
                            </pluginExecutions>
                        </lifecycleMappingMetadata>
                    </configuration>
                </plugin>
                <plugin>
                    <artifactId>maven-assembly-plugin</artifactId>
                    <groupId>org.apache.maven.plugins</groupId>
                    <version>2.4</version>
                </plugin>
                <plugin>
                    <artifactId>maven-compiler-plugin</artifactId>
                    <version>3.2</version>
                    <configuration>
                        <source>1.8</source>
                        <target>1.8</target>
                        <testSource>1.8</testSource>
                        <testTarget>1.8</testTarget>
                        <compilerVersion>1.8</compilerVersion>
                        <fork>true</fork>
                        <meminitial>512m</meminitial>
                        <maxmem>4096m</maxmem>
                        <compilerArgs>
                            <arg>-Xlint</arg>
                        </compilerArgs>
                    </configuration>
                </plugin>
                <plugin>
                    <artifactId>maven-dependency-plugin</artifactId>
                    <groupId>org.apache.maven.plugins</groupId>
                    <version>2.8</version>
                </plugin>
                <plugin>
                    <artifactId>maven-install-plugin</artifactId>
                    <groupId>org.apache.maven.plugins</groupId>
                    <version>2.5.2</version>
                </plugin>
                <plugin>
                    <artifactId>maven-jar-plugin</artifactId>
                    <version>2.5</version>
                    <executions>
                        <execution>
                            <id>api-docs-jar</id>
                            <goals>
                                <goal>jar</goal>
                            </goals>
                            <phase>package</phase>
                            <configuration>
                                <classesDirectory>src/main/docs</classesDirectory>
                                <classifier>api-docs</classifier>
                                <skipIfEmpty>true</skipIfEmpty>
                                <archive>
                                    <addMavenDescriptor>false</addMavenDescriptor>
                                </archive>
                            </configuration>
                        </execution>
                    </executions>
                </plugin>
                <plugin>
                    <artifactId>maven-war-plugin</artifactId>
                    <version>2.6</version>
                </plugin>
            </plugins>
        </pluginManagement>
    </build>

    <!--  Cross cutting dependencies are to be commonly maintained here -->
    <dependencies>
        <dependency>
            <groupId>org.mockito</groupId>
            <artifactId>mockito-core</artifactId>
            <scope>test</scope>
            <version>2.8.9</version>
        </dependency>
        <dependency>
            <groupId>junit</groupId>
            <artifactId>junit</artifactId>
            <version>4.13.1</version>
            <scope>test</scope>
        </dependency>
        <dependency>
            <artifactId>powermock-module-junit4</artifactId>
            <groupId>org.powermock</groupId>
            <scope>test</scope>
            <version>1.7.1</version>
        </dependency>
        <dependency>
            <artifactId>powermock-api-mockito2</artifactId>
            <groupId>org.powermock</groupId>
            <scope>test</scope>
            <version>1.7.1</version>
        </dependency>
        <dependency>
            <artifactId>powermock-core</artifactId>
            <groupId>org.powermock</groupId>
            <scope>test</scope>
            <version>1.7.1</version>
        </dependency>
        <dependency>
            <artifactId>jedis</artifactId>
           <groupId>redis.clients</groupId>
            <version>2.9.3</version>
        </dependency>
    </dependencies>

    <modules>
        <module>redfish-device-access-common</module>
        <module>redfish-device-access</module>
        <module>redfish-server-adapter-workflows</module>
        <module>redfish-server-adapter-app</module>
        <module>redfish-server-adapter-app-com</module>
        <module>redfish-device-access-com</module>
        <module>redfish-server-adapter-messaging-common</module>
        <module>redfish-server-adapter-messaging</module>
        <module>redfish-server-adapter-messaging-com</module>
        <module>redfish-server-adapter-model-common</module>
        <module>redfish-server-adapter-model</module>
        <module>redfish-server-adapter-model-com</module>
        <module>zinc-mock</module>
        <module>redfish-server-adapter-rpm</module>
    </modules>

</project>
